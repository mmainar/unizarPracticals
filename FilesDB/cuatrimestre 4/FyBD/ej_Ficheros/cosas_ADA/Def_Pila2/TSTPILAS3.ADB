with Gnat.Io, Defpila2; use Gnat.Io;procedure tstpilas2 is   package Pila_Ent is new Defpila2(Integer); use Pila_Ent;   Undato, i : Integer;   miPila : Pila2(100);begin   Put ("introduzca una secuencia de enteros terminada en 0: "); new_line;   i:=0;   loop     Get(Undato);   exit when Undato=0;     Pila_Ent.Apilar (miPila, Undato);     i:=i+1;   end loop;   new_line(2);   Put ("total de enteros introducidos: "); Put (i); new_line;   Put ("Secuencia de enteros introducida: "); new_line;   i:=0;   while not Pila_Ent.pilavacia(miPila) loop     Pila_Ent.desapilar(miPila, UnDato);     Put (UnDato); new_line;     i:=i+1;   end loop;   Put ("total de enteros guardados: "); Put (i); new_line;end tstpilas2;