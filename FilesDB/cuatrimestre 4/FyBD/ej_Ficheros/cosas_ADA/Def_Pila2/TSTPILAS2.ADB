with Text_Io, Defpila2; use Text_Io;procedure Tstpilas3 is   package Mi_Integer_Io is new Integer_Io(Integer); use Mi_Integer_Io;   package Mi_Float_Io is new Float_Io(Float); use Mi_Float_Io;   package Pila_Ent is new Defpila2(Integer); use Pila_Ent;   package Pila_Real is new Defpila2(Float); use Pila_Real;   Undato, I : Integer;   X : Float;   Mipila : Pila_Ent.Pila2(100);   Otrapila : Pila_Real.Pila2(100);begin   Put ("introduzca una secuencia de enteros terminada en 0: "); New_Line;   I:=0;   loop      Get(Undato);      exit when Undato=0;      Apilar (Mipila, Undato);      Apilar (Otrapila, Float(Undato));      I:=I+1;   end loop;   New_Line(2);   Put ("total de enteros introducidos: "); Put (I); New_Line;   Put ("Secuencia de enteros introducida: "); New_Line;   I:=0;   while not Pilavacia(Mipila) loop      Desapilar(Mipila, Undato);      Put (Undato); New_Line;      Desapilar(Otrapila, X);      Put (X); New_Line;      I:=I+1;   end loop;   Put ("total de enteros guardados: "); Put (I); New_Line;end Tstpilas3;