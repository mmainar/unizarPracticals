package body Defpila2 is   procedure Apilar (P: in out Pila2; E: in Elemento) is   begin      P.Puntpila:=P.Puntpila+1;      P.Datos(P.Puntpila):=E;   end Apilar;     procedure Desapilar (P: in out Pila2; E: out Elemento) is   begin      P.Puntpila:=P.Puntpila-1;      E:= P.Datos(P.Puntpila+1);   end Desapilar;   function Pilavacia (P: Pila2) return Boolean is   begin      return P.Puntpila<=0;   end Pilavacia;end Defpila2;