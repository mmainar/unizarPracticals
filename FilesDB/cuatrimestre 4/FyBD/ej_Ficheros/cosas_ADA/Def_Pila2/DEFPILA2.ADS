generictype Elemento is private;package Defpila2 is   type Pila2(Size : Positive) is limited private;   procedure Apilar (P: in out Pila2; E: in Elemento);   procedure Desapilar (P: in out Pila2; E: out Elemento);   function Pilavacia (P: Pila2) return Boolean;   Overflow, Underflow : exception;private   type Tabladatos is array (Positive range <>) of Elemento;   type Pila2(Size : Positive) is   record      Datos : Tabladatos(1..Size);      Puntpila : Natural := 0;   end record;end Defpila2;