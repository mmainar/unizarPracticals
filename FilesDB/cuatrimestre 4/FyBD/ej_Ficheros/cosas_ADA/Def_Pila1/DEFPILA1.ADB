package body Defpila1 is   type Tabladatos is array (Positive range <>) of Elemento;   Datos:Tabladatos(1..Size);   Puntpila:Natural := 0;   procedure Apilar (E: in Elemento) is   begin if Puntpila >= Size then raise Overflow;end if;      Puntpila:=Puntpila+1;      Datos(Puntpila):=E;   end Apilar;     procedure Desapilar (E: out Elemento) is   begin      if Puntpila <= 0 then raise Underflow;end if;      E:=Datos(Puntpila);      Puntpila:=Puntpila-1;   end Desapilar;   function Pilavacia return Boolean is   begin      return (Puntpila<=0);   end Pilavacia;end Defpila1;