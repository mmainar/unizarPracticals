with Gnat.Io, Defpila1; use Gnat.Io;procedure Tstpilas1 is   package Pila_Ent is new Defpila1(100,Integer); use Pila_Ent;   Undato, i : Integer;begin   Put ("introduzca una secuencia de enteros terminada en 0: "); new_line;   i:=0;   loop     Get(Undato);   exit when Undato=0;     Pila_Ent.Apilar (Undato);     i:=i+1;   end loop;   new_line(2);   Put ("total de enteros introducidos: "); Put (i); new_line;   Put ("Secuencia de enteros introducida: "); new_line;   i:=0;   while Pila_Ent.pilavacia=false loop     Pila_Ent.desapilar(UnDato);     Put (UnDato); new_line;     i:=i+1;   end loop;   Put ("total de enteros guardados: "); Put (i); new_line;end Tstpilas1;